    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/args2json/bin/Debug/net6.0/args2json.dll",
            "args": ["--version-pattern", "^5\\.0\\.\\3$", "--output", "./bin"],
            "cwd": "${workspaceFolder}/src/args2json",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}